class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        # a=len(nums)
        # for i in range(a):
        #     for j in range(i+1,a):
        #         if nums[i]== nums[j]:
        #             return True
        # return False 
        nums.sort()     
        for i in range(len(nums)-1):
            if nums[i]==nums[i+1]:
                return True
        return False
